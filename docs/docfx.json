{
  "metadata": [
    {
      "src": [
        {
          "src": "../package/SLTUtils",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "CDC.SoundLateralizationTask",
      "_appTitle": "CDC.SoundLateralizationTask",
      "_appLogoPath": "images/logo_white.svg",
      "_appFaviconPath":"images/logo.svg",
      "_enableSearch": true,
      "pdf": true
    }
  }
}